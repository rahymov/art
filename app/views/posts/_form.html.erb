<div class="container">
	<div class="row">
		<div class="col-md-6">
			<%= simple_form_for @post do |f| %>
				<%= f.input :image %>
				<%= f.input :title %>
				<%= f.input :link %>
				<%= f.input :description %>
				<%= f.button :submit ,class: "btn btn-primary" %>
			<% end %>
		</div>
	</div>
</div>
